package conc;

public class concatenation {
    public static void main(String[] args) {
        String str1 = "Welcome";
        str1 = str1.concat(" to ");
        str1 = str1.concat(" college");
        System.out.println(str1);
        String str2 = "This";
        str2 = str2.concat(" is").concat(" a").concat(" cat");
        System.out.println(str2);
    }
}
