package Hybrid_inheritance;

class C
{
    public void disp()
    {
        System.out.println("C");
    }
}

class A extends C
{
    public void disp()
    {
        System.out.println("A");
    }
}

class B extends C
{
    public void disp()
    {
        System.out.println("B");
    }

}

class D extends A
{
    public void disp()
    {
        System.out.println("D");
    }
    public static void main(String args[]){

        D obj = new D();
        obj.disp();
    }
}
