package comparetoignore;

import java.util.Scanner;

public class Compare_to_ignore {
    public static void main(String[] args) {
        Scanner vj=new Scanner(System.in);
        System.out.println("enter your 1st string");
        String str1=vj.nextLine().trim();
        System.out.println("enter your 2nd string");
        String str2=vj.nextLine().trim();
        int v=str1.compareToIgnoreCase(str2);
        System.out.println("result is "+v);
    }
}
