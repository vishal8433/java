package for_loop;

import java.util.Scanner;

public class for_loop {
    public static void main(String[] args) {
        Scanner vj=new Scanner(System.in);
        int a;
        System.out.println("enter your number");
        a=vj.nextInt();
        for(int i=1;i<=a;i++)
        {

            System.out.println(i);
        }
    }
}
