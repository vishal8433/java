package intro;

import java.util.Scanner;

public class intro {
    public static void main(String[] args) {
        String Name,gender;
        int age;
        Scanner vj=new Scanner(System.in);
        System.out.println("enter your name,age,gender");
        Name=vj.nextLine();
        gender=vj.nextLine();
        age=vj.nextInt();
        System.out.println("your intro is "+Name+" "+age+" "+gender);
    }

}
